<body>
	<!-- Refresh Button: for development purposes only; comment out for production -->
    <form><button class="btn btn-success" type="submit" onClick="history.go()"><i class="glyphicon glyphicon-refresh"></i></button></form> 
   
	<div class="container2">
		<div class="content">
			<div class="available-licenses">
				<h4>Available licenses</h4>
				<br />
				<span>TestExecute</span><span class="license-count" id="teLicenses">3</span>
			</div>
			<center>
				<div class="dropdown-tests">
					<select class="form-control" onChange="onTestSelectionChanged(this);">
						<option value="">Select a test...</option>
					    <option disabled="disabled">Desktop</option>
						<option disabled="disabled">&nbsp;&nbsp;Acceptance</option>
						<option value="Desktop Acceptance: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
						<option value="Desktop Acceptance: Test 2">&nbsp;&nbsp;&nbsp;Test 2</option>
						<option value="Desktop Acceptance: Test 3">&nbsp;&nbsp;&nbsp;Test 3</option>
						<option disabled="disabled">&nbsp;&nbsp;Performance</option>
						<option value="Desktop Performance: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
						<option value="Desktop Performance: Test 2">&nbsp;&nbsp;&nbsp;Test 2</option>
					    <option disabled="disabled">&nbsp;&nbsp;Regression</option>
						<option value="Desktop Regression: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
						<option value="Desktop Regression: Test 2">&nbsp;&nbsp;&nbsp;Test 2</option>
						<option disabled="disabled">---------------------------</option>
						<option disabled="disabled">Android</option>
					    <option disabled="disabled">&nbsp;&nbsp;Acceptance</option>
						<option value="Android Acceptance: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
					    <option disabled="disabled">&nbsp;&nbsp;Regression</option>
						<option value="Android Regression: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
						<option disabled="disabled">---------------------------</option>
						<option disabled="disabled">iOS</option>
						<option disabled="disabled">&nbsp;&nbsp;Acceptance</option>
						<option value="iOS Acceptance: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
					    <option disabled="disabled">&nbsp;&nbsp;Regression</option>
						<option value="iOS Regression: Test 1">&nbsp;&nbsp;&nbsp;Test 1</option>
					</select>
				</div>
				
				<!-- todo: add scroll bar for when list gets too big; and only visible when list is too big -->
				<table class="table-striped">
					<thead>
						<tr>
							<th class="text-center">#</th>
							<th class="text-center">Server Model</th>
							<th class="text-center">Computer Name</th>
							<th class="text-center">OS</th>
							<th class="text-center">Arch.</th>
							<th class="text-center">IP Address</th>
							<th class="text-center">Password</th>
							<th class="text-center">Status</th>
							<th id="tableHeaderTestName" class="text-center"></th>
						</tr>
					</thead>
					<tbody id="tableBody">
					</tbody>
				</table>
			</center>
		</div>
	</div>
