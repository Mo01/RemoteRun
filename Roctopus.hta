<script language="javascript" type="text/javascript">
	
	$(document).ready(function(){
		for (var i = 0; i < remoteMachines.length; i++) {
			var row = tableBody.insertRow(-1);
			for (var key in remoteMachines[i].htmlProperties) {
				var cell = row.insertCell(-1);
				cell.innerHTML = remoteMachines[i].htmlProperties[key];
			}

			if (!pingMachine2(remoteMachines[i].ip)) {
				document.getElementById("machineStatus" + i).innerHTML = "machine down";
			}
		}
	});
	
	function pingMachine2(ip) {
		var wshShell = new ActiveXObject("WScript.Shell");
		clearConsoleOutputInnerHtml();
		openConsoleOutputTag();
    	wshShell.Run('%comspec% /c ping ' + ip + ' -n 1 -w 100 >> ' + filePath, 0, true);
		closeConsoleOutputTag();
		alert(consoleOutput.innerHTML);
		clearConsoleOutputInnerHtml();
		return true;
	}
	
	function pingMachine(ip) {
		var wshShell = new ActiveXObject("WScript.Shell");
    	var pingOutput = wshShell.Exec('ping ' + ip + ' -n 1 -w 100 | Find "Reply"').StdOut.ReadAll();
		return !(pingOutput.indexOf("timed out") >= 0);
	}
	
	function onTxtPasswordKeyUp(id) {
		if (event.keyCode == 13) {
			remoteDesktop(id);
		}
	}
	
	function remoteDesktop(id) {
		var password = document.getElementById("txtPassword" + id).value;
		if (password == "" || password == undefined) {
			alert("Please provide a password");
			return;
		}

		var wshShell = new ActiveXObject("WScript.Shell");
		wshShell.Run('cmdkey /generic:' + remoteMachines[id].computerName + ' /user:"' + remoteMachines[id].username + '" /pass:"' + password + '"');
		wshShell.Run('mstsc /v:' + remoteMachines[id].computerName + ' /admin /fullscreen');

	}
	
	function onRunTest(id) {
		//todo: add logic to run test
		
	}
	
	function onChangeTest(element) {
		testToRun.innerHTML = sutSelection.value + ": " + element.value;
	}
	
	function clearConsoleOutputInnerHtml() {
		consoleOutput.innerHTML = "";
	}
	
	function openConsoleOutputTag() {
		var fso = new ActiveXObject("Scripting.FileSystemObject");

		var fh = fso.OpenTextFile(filePath);
		var lines = fh.ReadAll();
		fh.Close();
		
		lines = lines.split('\r\n');
		lines = lines.slice(0, -1);
		lines = lines.join('\r\n');

		var fh = fso.CreateTextFile(filePath, true);
		fh.Write(lines);
		fh.Close();
	}
	
	function closeConsoleOutputTag() {
		var fso = new ActiveXObject("Scripting.FileSystemObject");

		var fh = fso.OpenTextFile(filePath);
		var lines = fh.ReadAll();
		fh.Close();
		
		// todo: need to play with regex a little more; need to find a way to not get this line replace at all
		lines = lines.replace(/(Reply from .* time<1ms .*)/g, "Success");
		
		lines = lines.split('\r\n');
		lines.push('</consoleOutput>');
		lines = lines.join('\r\n');

		var fh = fso.CreateTextFile(filePath, true);
		fh.Write(lines);
		fh.Close();
	}

</script>

